    "eslint": "^2.9.0",
    "eslint-config-arenanet": "^3.0.1",
    "functional-dom-query": "file:esdoc",
    "jsdom": "^9.8.3",
    "jsdom-global": "2.1.0",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0",
